    {
      "cell_type": "code",
      "source": [
        "from sklearn.model_selection import cross_val_score\n",
        "import numpy as np\n",
        "\n",
        "X_rd = X[[\"R&D Spend\"]]\n",
        "model = LinearRegression()\n",
        "r2_rd = cross_val_score(model, X_rd, y, cv=5, scoring=\"r2\")\n",
        "print(\"只用 R&D Spend 的 R² 分數：\", r2_rd)\n",
        "print(\"平均 R²：\", np.mean(r2_rd))"
