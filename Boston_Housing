      "cell_type": "code",
      "source": [
        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "import warnings\n",
        "warnings.filterwarnings('ignore')\n",
        "\n",
        "# 只選數值欄位畫 pairplot（One-Hot 編碼的 State 欄位是數值0/1也會畫）\n",
        "sns.pairplot(X)\n",
        "plt.suptitle(\"特徵兩兩散點圖矩陣\", y=1.02)\n",
        "plt.show()"
      ],
      "metadata": {
