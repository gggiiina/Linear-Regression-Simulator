    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "from sklearn.preprocessing import OneHotEncoder\n",
        "\n",
        "# 讀資料\n",
        "url = \"https://gist.githubusercontent.com/GaneshSparkz/b5662effbdae8746f7f7d8ed70c42b2d/raw/faf8b1a0d58e251f48a647d3881e7a960c3f0925/50_Startups.csv\"\n",
        "df = pd.read_csv(url)\n",
        "\n",
        "# One-Hot Encoding\n",
        "X = df.drop(\"Profit\", axis=1)\n",
        "y = df[\"Profit\"]\n",
        "categorical_cols = [\"State\"]\n",
        "encoder = OneHotEncoder(drop='first', sparse_output=False)\n",
        "encoded_states = encoder.fit_transform(X[categorical_cols])\n",
        "encoded_state_df = pd.DataFrame(encoded_states, columns=encoder.get_feature_names_out(categorical_cols))\n",
        "X = X.drop(categorical_cols, axis=1)\n",
        "X = pd.concat([X, encoded_state_df], axis=1)"
      ],
      "metadata": {
        "id": "zUQ-GBM4AUPa"
      },
      "execution_count": 6,
      "outputs": []
    },
    {
      "cell_type": "markdown",
