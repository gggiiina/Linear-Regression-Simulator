      "cell_type": "code",
      "source": [
        "from sklearn.preprocessing import StandardScaler\n",
        "\n",
        "scaler = StandardScaler()\n",
        "X_scaled = scaler.fit_transform(X)\n",
        "model2.fit(X_scaled, y)\n",
        "\n",
        "importance = pd.Series(model2.coef_, index=X.columns).sort_values(key=abs, ascending=False)\n",
        "print(\"標準化後的特徵重要性（可比較影響力）\")\n",
        "print(importance)"
      ],
